// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model MachineLog {
  id        Int      @id @default(autoincrement())
  machineId String
  timestamp DateTime @default(now())
  isInUse   Boolean
}
